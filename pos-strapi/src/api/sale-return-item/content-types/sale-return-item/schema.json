{
  "kind": "collectionType",
  "collectionName": "sale_return_items",
  "info": {
    "singularName": "sale-return-item",
    "pluralName": "sale-return-items",
    "displayName": "Sale Return Item"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer"
    },
    "price": {
      "type": "decimal"
    },
    "total": {
