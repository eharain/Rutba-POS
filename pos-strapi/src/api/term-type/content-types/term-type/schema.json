{
  "kind": "collectionType",
  "collectionName": "term_types",
  "info": {
    "singularName": "term-type",
    "pluralName": "term-types",
    "displayName": "Term Type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "terms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::term.term",
      "inversedBy": "term_types"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "is_variant": {
      "type": "boolean"
    },
    "is_public": {
      "type": "boolean"
    }
  }
}
