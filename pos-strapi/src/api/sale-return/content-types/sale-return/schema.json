{
  "kind": "collectionType",
  "collectionName": "sale_returns",
  "info": {
    "singularName": "sale-return",
    "pluralName": "sale-returns",
    "displayName": "Sale Return"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "return_no": {
      "type": "string",
      "required": true
    },
    "return_date": {
      "type": "datetime",
      "required": true
    },
    "total_refund": {
      "type": "decimal"
    },
    "type": {
      "type": "enumeration",
      "enum": ["Return", "Exchange"],
      "default": "Return"
    },
    "exchange_sale": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sale.sale"
    },
    "sale": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sale.sale"
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sale-return-item.sale-return-item",
      "mappedBy": "sale_return"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "sale_return"
    },
    "cash_register": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cash-register.cash-register"
    },
    "refund_method": {
      "type": "enumeration",
      "enum": ["Cash", "Card", "Bank", "Mobile Wallet", "Exchange Return", "Store Credit"],
      "default": "Cash"
    },
    "refund_status": {
      "type": "enumeration",
      "enum": ["Pending", "Refunded", "Credited"],
      "default": "Pending"
    },
    "owners": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user"
    },
    "branches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::branch.branch",
      "mappedBy": "sale_returns"
    }
  }
}
