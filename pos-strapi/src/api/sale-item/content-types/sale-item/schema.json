{
  "kind": "collectionType",
  "collectionName": "sale_items",
  "info": {
    "singularName": "sale-item",
    "pluralName": "sale-items",
    "displayName": "Sale Item"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer"
    },
    "price": {
      "type": "decimal"
    },
    "discount": {
      "type": "decimal"
    },
    "tax": {
      "type": "decimal"
    },
    "total": {
      "type": "decimal"
    },
    "sale": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sale.sale",
      "inversedBy": "items"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product"
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stock-item.stock-item",
      "mappedBy": "sale_item"
    }
  }
}
