{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "displayName": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "isStaff": {
      "type": "boolean"
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "sales": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sale.sale",
      "inversedBy": "users"
    },
    "purchases": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::purchase.purchase",
      "inversedBy": "users"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "users"
    },
    "sale_returns": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sale-return.sale-return",
      "inversedBy": "users"
    },
    "purchase_returns": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::purchase-return.purchase-return",
      "inversedBy": "users"
    }
  }
}
