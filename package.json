    "build:accounts": "node scripts/load-env.js -- npm run build --workspace=rutba-accounts",
    "build:payroll": "node scripts/load-env.js -- npm run build --workspace=rutba-payroll",
    "build:cms": "node scripts/load-env.js -- npm run build --workspace=rutba-cms",
    "build:all": "npm run build:auth && npm run build:stock && npm run build:sale && npm run build:web && npm run build:web-user && npm run build:crm && npm run build:hr && npm run build:accounts && npm run build:payroll && npm run build:cms",
    "docker:env": "node scripts/generate-docker-env.js"
