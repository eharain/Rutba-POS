{
  "private": true,
  "workspaces": [
    "packages/*",
    "pos-desk",
    "pos-stock",
    "pos-sale"
  ],
  "scripts": {
    "dev:desk": "npm run dev --workspace=pos-desk",
    "dev:stock": "npm run dev --workspace=pos-stock",
    "dev:sale": "npm run dev --workspace=pos-sale",
    "build:stock": "npm run build --workspace=pos-stock",
    "build:sale": "npm run build --workspace=pos-sale",
    "build:all": "npm run build --workspace=pos-stock && npm run build --workspace=pos-sale"
  },
  "dependencies": {
    "marked": "^17.0.1"
  }
}
