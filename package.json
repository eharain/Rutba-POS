    "dev:crm": "node scripts/load-env.js -- npm run dev --workspace=rutba-crm",
    "dev:hr": "node scripts/load-env.js -- npm run dev --workspace=rutba-hr",
    "dev:accounts": "node scripts/load-env.js -- npm run dev --workspace=rutba-accounts",
    "dev:payroll": "node scripts/load-env.js -- npm run dev --workspace=rutba-payroll",
    "dev:cms": "node scripts/load-env.js -- npm run dev --workspace=rutba-cms",
    "build:strapi": "node scripts/load-env.js -- npm --prefix pos-strapi run build",
    "build:auth": "node scripts/load-env.js -- npm run build --workspace=pos-auth",
