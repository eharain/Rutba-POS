    "pos-stock",
    "pos-sale",
    "pos-auth",
    "rutba-web",
    "rutba-web-user",
    "rutba-crm",
    "rutba-hr",
    "rutba-accounts",
    "rutba-payroll",
    "rutba-cms"
  ],
  "scripts": {
    "dev:desk": "node scripts/load-env.js -- npm run dev --workspace=pos-desk",
    "dev:strapi": "node scripts/load-env.js -- npm --prefix pos-strapi run develop",
    "dev:auth": "node scripts/load-env.js -- npm run dev --workspace=pos-auth",
